"Unlittwotexture"
{
	"$baseTexture" "hl2ctf/security/df_security_screens"
	"$texture2" "hl2ctf/security/df_screen_noise"
	"$selfillum" 1
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture" 
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" .3
		}
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 10
			"texturescrollangle" 70
		}
	}
}
