"LightmappedGeneric"
{
	"$basetexture" "hl2ctf/tile/tilefloor001b_c"
	"$surfaceprop" "tile"
	"%keywords" "hl2ctf"
	"$bumpmap" "hl2ctf/tile/tilefloor001b_normal"
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" 1
	"$envmaptint" "[ .80 .80 .80 ]"
	"$fresnelreflection" 0.25
	"$texscale"	2
	"$bumpscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
     	{
            "TextureTransform"
            {
		   "translateVar" "$baseTextureOffset"
		   "scaleVar"     "$texscale"
                   "resultVar"    "$baseTextureTransform"

            }
            "TextureTransform"
            {
		   "translateVar" "$baseTextureOffset"
		   "scaleVar"     "$texscale"
                   "resultVar"    "$bumptransform"
            }
     	}
}
