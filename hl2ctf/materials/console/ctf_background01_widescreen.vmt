"unlittwotexture"
{
	"$baseTexture" "console/ctf_background01_widescreen"
	"$texture2" "console/ctf_background_noise"
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$ignorez" 1
	"$no_fullbright" "1"
	"$nolod" "1"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture" 
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 30
		}
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 1
			"texturescrollangle" 70
		}
	}
}